<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>HRC_intern</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>FetchData</servlet-name>
    <servlet-class>com.Highradius.FetchData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FetchData</servlet-name>
    <url-pattern>/fetch</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddInvoice</servlet-name>
    <servlet-class>com.Highradius.AddInvoice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddInvoice</servlet-name>
    <url-pattern>/ADD</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteInvoice</servlet-name>
    <servlet-class>com.Highradius.DeleteInvoice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteInvoice</servlet-name>
    <url-pattern>/DELETE</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditInvoice</servlet-name>
    <servlet-class>com.Highradius.EditInvoice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditInvoice</servlet-name>
    <url-pattern>/Edit</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SearchInvoice</servlet-name>
    <servlet-class>com.Highradius.SearchInvoice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchInvoice</servlet-name>
    <url-pattern>/Search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdvanceSearch</servlet-name>
    <servlet-class>com.Highradius.AdvanceSearch</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdvanceSearch</servlet-name>
    <url-pattern>/Advance</url-pattern>
  </servlet-mapping>
</web-app>